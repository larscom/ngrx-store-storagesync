{
  "name": "ngrx-store-storagesync-app",
  "scripts": {
    "ng": "ng",
    "test": "jest --detectOpenHandles",
    "test:ci": "jest --runInBand --detectOpenHandles --coverage",
    "test:watch": "jest --detectOpenHandles --watch",
    "test:coverage": "jest --detectOpenHandles --coverage",
    "e2e": "ng e2e",
    "e2e:ci": "npm run e2e -- --devServerTarget= --baseUrl=http://127.0.0.1 --webdriver-update=false",
    "start": "npm run start:app",
    "start:app": "ng serve",
    "start:ssr": "ng run ngrx-store-storagesync-app:serve-ssr",
    "build:lib": "ng build ngrx-store-storagesync && npm run copy:readme",
    "build:app": "ng build --prod",
    "build:app-ssr": "ng build --prod && ng run ngrx-store-storagesync-app:server:production",
    "build:app-prerender": "ng run ngrx-store-storagesync-app:prerender",
    "serve:ssr": "node dist/ngrx-store-storagesync-app/server/main.js",
    "copy:readme": "cpx README.md dist/ngrx-store-storagesync",
    "postinstall": "mkdir -p projects/ngrx-store-storagesync/.git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.5",
    "@angular/cdk": "^12.0.5",
    "@angular/common": "^12.0.5",
    "@angular/compiler": "^12.0.5",
    "@angular/core": "^12.0.5",
    "@angular/forms": "^12.0.5",
    "@angular/material": "^12.0.5",
    "@angular/platform-browser": "^12.0.5",
    "@angular/platform-browser-dynamic": "^12.0.5",
    "@angular/platform-server": "^12.0.5",
    "@angular/router": "^12.0.5",
    "@ngrx/store": "^12.1.0",
    "@ngrx/store-devtools": "^12.1.0",
    "@nguniversal/express-engine": "^12.0.2",
    "express": "^4.17.1",
    "lodash-es": "^4.14.149",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "uuid": "^8.3.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.5",
    "@angular/cli": "^12.0.5",
    "@angular/compiler-cli": "^12.0.5",
    "@angular/language-service": "^12.0.5",
    "@larscom/cpx": "^2.0.1",
    "@nguniversal/builders": "^10.0.1",
    "@types/express": "^4.17.6",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^26.0.22",
    "@types/lodash-es": "^4.14.149",
    "@types/node": "^13.11.1",
    "@types/uuid": "^8.0.1",
    "codelyzer": "^6.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "ng-packagr": "^12.0.6",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.39.1",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
}