{
  "name": "ngrx-store-storagesync-app",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:lib": "ng build ngrx-store-storagesync && npm run copy:readme",
    "watch": "ng build --watch --configuration development",
    "test": "ng test ngrx-store-storagesync",
    "test:ci": "ng test ngrx-store-storagesync --watch=false --browsers ChromeHeadless --code-coverage",
    "copy:readme": "cpx README.md dist/ngrx-store-storagesync",
    "postinstall": "mkdir -p projects/ngrx-store-storagesync/.git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.1",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^15.1.1",
    "@angular/compiler": "^15.1.1",
    "@angular/core": "^15.1.1",
    "@angular/forms": "^15.1.1",
    "@angular/material": "^14.2.0",
    "@angular/platform-browser": "^15.1.1",
    "@angular/platform-browser-dynamic": "^15.1.1",
    "@angular/router": "^15.1.1",
    "@ngrx/store": "^14.3.2",
    "@ngrx/store-devtools": "^14.3.2",
    "ramda": "^0.28.0",
    "rxjs": "^7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.2",
    "@angular/cli": "^15.1.2",
    "@angular/compiler-cli": "^15.1.1",
    "@larscom/cpx": "^2.1.0",
    "@types/jasmine": "~4.0.0",
    "@types/ramda": "^0.28.18",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.1.1",
    "typescript": "~4.9.4"
  }
}
