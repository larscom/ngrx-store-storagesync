{
  "name": "ngrx-store-storagesync-app",
  "scripts": {
    "ng": "ng",
    "test": "jest --detectOpenHandles --coverage",
    "test:ci": "jest --forceExit --runInBand --detectOpenHandles --coverage",
    "test:watch": "jest --detectOpenHandles --watch",
    "test:coverage": "jest --detectOpenHandles --coverage",
    "e2e": "bash e2e/run.sh",
    "start": "npm run start:app",
    "start:app": "ng serve",
    "start:ssr": "ng run ngrx-store-storagesync-app:serve-ssr",
    "build:lib": "ng build ngrx-store-storagesync && npm run copy:readme",
    "build:app": "ng build --configuration production",
    "build:app-ssr": "ng build --configuration production && ng run ngrx-store-storagesync-app:server:production",
    "build:app-prerender": "ng run ngrx-store-storagesync-app:prerender",
    "serve:ssr": "node dist/ngrx-store-storagesync-app/server/main.js",
    "copy:readme": "cpx README.md dist/ngrx-store-storagesync",
    "postinstall": "mkdir -p projects/ngrx-store-storagesync/.git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^13.0.2",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/material": "^13.0.2",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/platform-server": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@ngrx/store": "^13.0.1",
    "@ngrx/store-devtools": "^13.0.1",
    "@nguniversal/express-engine": "^13.0.1",
    "express": "^4.17.1",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular/cli": "^14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@angular/language-service": "^14.0.0",
    "@larscom/cpx": "^2.1.0",
    "@nguniversal/builders": "^13.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.9",
    "@types/uuid": "^8.3.3",
    "codecov": "^3.8.3",
    "codelyzer": "^6.0.2",
    "jest": "^27.3.1",
    "jest-preset-angular": "^11.0.0",
    "ng-packagr": "^14.0.0",
    "ts-node": "~7.0.0",
    "typescript": "~4.7.3"
  }
}