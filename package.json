{
  "name": "ngrx-store-storagesync-app",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:app",
    "start:app": "ng serve",
    "start:ssr": "ng run ngrx-store-storagesync-app:serve-ssr",
    "watch": "ng build --watch --configuration development",
    "e2e": "bash e2e/run.sh",
    "test": "ng test ngrx-store-storagesync",
    "test:ci": "ng test ngrx-store-storagesync --watch=false --browsers ChromeHeadless --code-coverage",
    "build:lib": "ng build ngrx-store-storagesync && npm run copy:readme",
    "build:app": "ng build --configuration production",
    "build:app-ssr": "ng build --configuration production && ng run ngrx-store-storagesync-app:server:production",
    "build:app-prerender": "ng run ngrx-store-storagesync-app:prerender",
    "serve:ssr": "node dist/ngrx-store-storagesync-app/server/main.js",
    "copy:readme": "cpx README.md dist/ngrx-store-storagesync",
    "postinstall": "mkdir -p projects/ngrx-store-storagesync/.git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/platform-server": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@ngrx/store": "^14.3.2",
    "@ngrx/store-devtools": "^14.3.2",
    "@nguniversal/express-engine": "^13.0.1",
    "express": "^4.17.1",
    "rxjs": "^7.5.0",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.7",
    "@angular/cli": "^14.2.7",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "@larscom/cpx": "^2.1.0",
    "@nguniversal/builders": "^13.0.1",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.178",
    "@types/uuid": "^8.3.3",
    "codecov": "^3.8.3",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.2.0",
    "typescript": "~4.7.3"
  }
}
